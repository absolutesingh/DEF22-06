package coreJava;

class Student
{
	int rollNo;
	String name;
}

public class ConstructorStudy {

	public static void main(String[] args) {
		Student s1 = new Student();
		System.out.println(s1.rollNo);
		System.out.println(s1.name);
		s1.rollNo = 1;
		s1.name = "Ram";
		System.out.println(s1.rollNo);
		System.out.println(s1.name);
		
	}

}
