package stacks;

class MyStack{
	final int size = 10;
	int arr[] = new int[size];
	int top = -1;
	
	boolean isEmpty() {
		return top == -1;
	}
	
	int peek() {
		return arr[top];
	}
	
	void push(int data)
	{
		top = top + 1;
		arr[top] = data;
	}
	
	int pop()
	{
		int x = arr[top];
		top = top - 1;
		return x;
	}
}

public class ArrayImp {

	public static void main(String[] args) {
		MyStack stack = new MyStack();
		
	}

}
