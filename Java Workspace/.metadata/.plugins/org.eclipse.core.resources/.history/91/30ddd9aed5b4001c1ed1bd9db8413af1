package coreJava;

public class LoopStudy {

	public static void main(String[] args) {
//		System.out.println("1");
//		System.out.println("2");
//		System.out.println("3");
		
		//FOR Loop
		
		int i;
		for(i=1;i<=100;i++)
		{
			System.out.println(i);
		}
		
		System.out.println("Value of i after loop is: " + i);
	}

}
