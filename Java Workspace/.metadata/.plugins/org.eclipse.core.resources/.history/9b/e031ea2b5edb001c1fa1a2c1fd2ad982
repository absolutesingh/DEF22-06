package trees;

import java.util.*;

class BSTNode {
	int data;
	BSTNode left;
	BSTNode right;

	BSTNode(int data) {
		this.data = data;
		this.left = null;
		this.right = null;
	}

}

public class BST {

	BSTNode root; // Root of the Binary Tree

	BST(int rootData) {
		this.root = new BSTNode(rootData);
	}



	public static void main(String[] args) {
		BST bst = new BST(15);
		bst.root.left = new BSTNode(3);
		bst.root.right = new BSTNode(5);
		bst.root.left.right = new BSTNode(9);
		bst.root.right.left = new BSTNode(7);
		// Sample tree has been created

	}

}
